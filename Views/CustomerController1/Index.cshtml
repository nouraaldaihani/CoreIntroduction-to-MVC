@{
}
@model List<WebApplication2.Models.Customer>
<h2>Customer</h2>
<table class="table">
<thead>
    <tr>
    <th>Id</th>
     <th>Name</th>
     <th>Phone</th>
    </tr>

</thead>

<tbody>

    @foreach (var c in Model)
        {
            <tr>
                <td>@c.Id</td>
                <td>@c.Name</td>
                <td>@c.Phone</td>
            </tr>

        }

</tbody>
</<table>
   